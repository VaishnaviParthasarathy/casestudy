package productDetail.model;

public class ProductDescription {
	
	private String genre;
	private String name;
	
	

	public ProductDescription() {
		super();
	}

	public ProductDescription(String genre, String name) {
		super();
		this.genre = genre;
		this.name = name;
	}
	
	public String getGenre() {
		return genre;
	}

	public void setGenre(String genre) {
		this.genre = genre;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	

}
